<div class="container">
  <div class="row">

    <h3>Users</h3>
    <div class="column">
      <table class="table">
        <tbody>
          <% @users.each do |user| %>
            <tr>
              <%= render user %>
            </tr>
          <% end %>
        </tbody>
      </table>
    </div>
  </div>

  <div class="row">
    <h4>Total Partner Revenue By Country</h4>
    <%= pie_chart @partners.group(:country).sum(:revenue_size)%>
    <div class="col-md-8 col-md-offset-2">
    </div>
  </div>
</div>
