<div id="ticker-bar" class="panel">
	<div class="panel-heading"><h3>Ticker Bar</h3></div>
	<div class="panel-body">
			<div class="col-small">
				<h5>Total Revenue</h5>
				<h4><%= @totalRevenue %></h4>
			</div>
			<div class="col-small">
				<h5>Average Revenue Per User</h5>
				<h4><%= @averageRevenue %></h4>
			</div>
			<div class="col-small">
				<h5>Total Costs</h5>
				<h4><%= @totalCosts %></h4>
			</div>
			<div class="col-small">
				<h5>Total Income</h5>
				<h4>
					<div class="input-group">
         		<span class="input-group-addon">$</span>
         		<input type="text" class="form-control" placeholder="Enter Amount Here">
      		</div>
    	</h4>
			</div>
	</div>
</div>